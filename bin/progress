#!/usr/bin/env python3.8

import sys


def progress(value, length, filled="█", empty="░"):
    if value < 0 or value > 1:
        raise ValueError("input outside range")
    n = int(value * length)
    sys.stdout.write("\r" + filled * n + empty * (length - n))
    sys.stdout.flush()


if __name__ == '__main__':
    kwargs = {}
    if len(sys.argv) == 5:
        kwargs = {"filled": sys.argv[3], "empty": sys.argv[4]}
    progress(float(sys.argv[1]), int(sys.argv[2]), **kwargs)
